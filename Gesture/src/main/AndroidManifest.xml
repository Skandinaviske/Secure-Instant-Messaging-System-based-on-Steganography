<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="pack.GestureApp"
>


    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity
            android:name="GestureActivity"
            android:label="@string/test_gesture_activity_label">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <!--category android:name="android.intent.category.LAUNCHER"/-->
            </intent-filter>
        </activity>
        <activity
            android:name=".SaveGestureActivity"
            android:label="@string/save_gesture">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <!--category android:name="android.intent.category.LAUNCHER"/-->
            </intent-filter>
        </activity>
        <activity android:name=".GestureListActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
    </application>

</manifest>